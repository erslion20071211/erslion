n style="COLOR:blue;FONT-FAMILY:'Courier New';"&gt; ..&lt;/span&gt;&lt;/font&gt;&lt;font color="blue"&gt;&lt;span style="COLOR:blue;"&gt;嗚&lt;/span&gt;&lt;/font&gt;&lt;font face="Courier New" color="blue"&gt;&lt;span style="COLOR:blue;FONT-FAMILY:'Courier New';"&gt; ...)&lt;/span&gt;&lt;/font&gt;&lt;font face="Arial" color="blue"&gt;&lt;span style="COLOR:blue;FONT-FAMILY:Arial;"&gt; &lt;br&gt;&lt;br&gt;&lt;br&gt;&lt;/span&gt;&lt;/font&gt;&lt;span&gt;&lt;img height="50" src="http://tw.mg30.mail.yahoo.com/ya/download?mid=1%5f1138%5f4938%5fADPJvMsAAD53SF2wNguoiUB3PRg&amp;amp;pid=12&amp;amp;fid=%2540B%2540Bulk&amp;amp;inline=1" width="50"/&gt;&lt;/span&gt;&lt;strong&gt;&lt;font color="purple" size="4"&gt;&lt;span style="FONT-WEIGHT:bold;FONT-SIZE:13.5pt;COLOR:purple;"&gt;祝大家工作順心！&lt;/span&gt;&lt;/font&gt;&lt;/strong&gt;&lt;strong&gt;&lt;font face="Times New Roman" color="blue"&gt;&lt;span style="FONT-WEIGHT:bold;COLOR:blue;FONT-FAMILY:'Times New Roman';"&gt; &lt;/span&gt;&lt;/font&gt;&lt;/strong&gt;&lt;/p&gt;
