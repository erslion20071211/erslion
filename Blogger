&lt;p&gt;&lt;span style="FONT-SIZE:14pt;"&gt;1.&lt;/span&gt;&lt;span style="FONT-SIZE:14pt;"&gt;傷膝蓋&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
&lt;p&gt;&lt;span style="FONT-SIZE:14pt;"&gt;2.&lt;/span&gt;&lt;span style="FONT-SIZE:14pt;"&gt;易傷腳踝&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td style="BORDER-RIGHT:windowtext 1pt solid;BORDER-LEFT-COLOR:#f0f0f0;BORDER-TOP-COLOR:#f0f0f0;HEIGHT:98.1pt;BACKGROUND-COLOR:transparent;"&gt;
&lt;p align="center"&gt;&lt;span style="FONT-SIZE:14pt;"&gt;95%&lt;/span&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td style="BORDER-RIGHT:windowtext 1pt solid;BORDER-LEFT-COLOR:#f0f0f0;BORDER-TOP-COLOR:#f0f0f0;HEIGHT:98.1pt;BACKGROUND-COLOR:transparent;"&gt;
&lt;p align="center"&gt;&lt;span style="FONT-SIZE:14pt;"&gt;30M&lt;/span&gt;&lt;span style="FONT-SIZE:14pt;"&gt;~2H&lt;/span&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td style="BORDER-RIGHT:windowtext 1pt solid;BORDER-LEFT-COLOR:#f0f0f0;BORDER-TOP-COLOR:#f0f0f0;HEIGHT:98.1pt;BACKGROUND-COLOR:transparent;"&gt;
