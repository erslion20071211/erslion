&lt;td style="BORDER-RIGHT:#c0c0c0;PADDING-RIGHT:0cm;BORDER-TOP:#c0c0c0;PADDING-LEFT:0cm;PADDING-BOTTOM:0cm;BORDER-LEFT:#c0c0c0;PADDING-TOP:0cm;BORDER-BOTTOM:#c0c0c0;HEIGHT:36pt;BACKGROUND-COLOR:transparent;" colSpan="2"&gt;
&lt;p class="MsoNormal" style="MARGIN:0cm 0cm 0pt;"&gt;&lt;font style="BACKGROUND-COLOR:#d0d0d0;"&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:white;FONT-FAMILY:Arial;"&gt;&lt;/span&gt;&lt;/strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:9.5pt;FONT-FAMILY:Arial;"&gt;&lt;/span&gt;&lt;/font&gt;&lt;/p&gt;
