&lt;tr&gt;
&lt;td style="BORDER-RIGHT:#c0c0c0;PADDING-RIGHT:0cm;BORDER-TOP:#c0c0c0;PADDING-LEFT:0cm;PADDING-BOTTOM:0cm;BORDER-LEFT:#c0c0c0;PADDING-TOP:0cm;BORDER-BOTTOM:#c0c0c0;BACKGROUND-COLOR:transparent;"&gt;
&lt;p class="MsoNormal" style="MARGIN:0cm 0cm 0pt;"&gt;&lt;span lang="EN-US" style="FONT-SIZE:9.5pt;FONT-FAMILY:Arial;"&gt;&lt;font style="BACKGROUND-COLOR:#d0d0d0;"&gt;&lt;/font&gt;&lt;/span&gt;&amp;nbsp;&lt;/p&gt;
&lt;/td&gt;
&lt;td style="BORDER-RIGHT:#c0c0c0;PADDING-RIGHT:0cm;BORDER-TOP:#c0c0c0;PADDING-LEFT:0cm;PADDING-BOTTOM:0cm;BORDER-LEFT:#c0c0c0;PADDING-TOP:0cm;BORDER-BOTTOM:#c0c0c0;BACKGROUND-COLOR:transparent;"&gt;
&lt;p class="MsoNormal" style="MARGIN:0cm 0cm 0pt;"&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:red;FONT-FAMILY:新細明體;"&gt;&lt;font style="BACKGROUND-COLOR:#d0d0d0;"&gt;報到時間：&lt;/font&gt;&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:9.5pt;FONT-FAMILY:Arial;"&gt;&lt;br&gt;&lt;/span&gt;&lt;/strong&gt;&lt;font style="BACKGROUND-COLOR:#d0d0d0;"&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:red;FONT-FAMILY:新細明體;"&gt;　&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:Arial;"&gt;2008&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:新細明體;"&gt;年&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:Arial;"&gt;7&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:新細明體;"&gt;月&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:Arial;"&gt;4&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:新細明體;"&gt;日&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:新細明體;"&gt;（星期五）&lt;/span&gt;&lt;/strong&gt;&lt;/font&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:Arial;"&gt;&lt;br&gt;&lt;/span&gt;&lt;/strong&gt;&lt;font style="BACKGROUND-COLOR:#d0d0d0;"&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;　&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:Arial;"&gt;13&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;：&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:Arial;"&gt;00&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;－&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:Arial;"&gt;17&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;：&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:Arial;"&gt;00&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;（鐵三角接力及半程賽）。&lt;/span&gt;&lt;/strong&gt;&lt;/font&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:Arial;"&gt;&lt;br&gt;&lt;/span&gt;&lt;/strong&gt;&lt;font style="BACKGROUND-COLOR:#d0d0d0;"&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;　&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:Arial;"&gt;2008&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:新細明體;"&gt;年&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:Arial;"&gt;7&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:新細明體;"&gt;月&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:Arial;"&gt;5&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:新細明體;"&gt;日&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:新細明體;"&gt;（星期六）&lt;/span&gt;&lt;/strong&gt;&lt;/font&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:Arial;"&gt;&lt;br&gt;&lt;/span&gt;&lt;/strong&gt;&lt;font style="BACKGROUND-COLOR:#d0d0d0;"&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;　&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:Arial;"&gt;13&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;：&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:Arial;"&gt;00&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;－&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:Arial;"&gt;17&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;：&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:Arial;"&gt;00&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;（標準賽程）。&lt;/span&gt;&lt;/strong&gt;&lt;/font&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:Arial;"&gt;&lt;br&gt;&lt;br&gt;&lt;/span&gt;&lt;/strong&gt;&lt;font style="BACKGROUND-COLOR:#d0d0d0;"&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:red;FONT-FAMILY:新細明體;"&gt;報到地點：&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;（大會將於賽前一周郵寄參賽通知）&lt;/span&gt;&lt;/strong&gt;&lt;/font&gt;&lt;font style="BACKGROUND-COLOR:#d0d0d0;"&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:Arial;"&gt; &lt;br&gt;&lt;br&gt;&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:red;FONT-FAMILY:新細明體;"&gt;競賽時間：&lt;/span&gt;&lt;/strong&gt;&lt;/font&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:9.5pt;FONT-FAMILY:Arial;"&gt;&lt;br&gt;&lt;/span&gt;&lt;/strong&gt;&lt;font style="BACKGROUND-COLOR:#d0d0d0;"&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:新細明體;"&gt;　&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:Arial;"&gt;2008&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:新細明體;"&gt;年&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:Arial;"&gt;7&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:新細明體;"&gt;月&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:Arial;"&gt;5&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:新細明體;"&gt;日&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:新細明體;"&gt;（星期六）&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;鐵三角接力及半程賽。&lt;/span&gt;&lt;/strong&gt;&lt;/font&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:Arial;"&gt;&lt;br&gt;&lt;/span&gt;&lt;/strong&gt;&lt;font style="BACKGROUND-COLOR:#d0d0d0;"&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;　&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:Arial;"&gt;2008&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:新細明體;"&gt;年&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:Arial;"&gt;7&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:新細明體;"&gt;月&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:Arial;"&gt;6&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:新細明體;"&gt;日&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:blue;FONT-FAMILY:新細明體;"&gt;（星期日）&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;標準賽程（&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:Arial;"&gt;51.5&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;公里&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;）。&lt;/span&gt;&lt;/strong&gt;&lt;/font&gt;&lt;font style="BACKGROUND-COLOR:#d0d0d0;"&gt;&lt;span lang="EN-US" style="FONT-SIZE:9.5pt;FONT-FAMILY:Arial;"&gt; &lt;br&gt;&lt;br&gt;&lt;/span&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:red;FONT-FAMILY:新細明體;"&gt;競賽地點：&lt;/span&gt;&lt;/strong&gt;&lt;/font&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:9.5pt;FONT-FAMILY:Arial;"&gt;&lt;br&gt;&lt;/span&gt;&lt;/strong&gt;&lt;font style="BACKGROUND-COLOR:#d0d0d0;"&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;　南投縣日月潭朝霧碼頭及環湖道路（台&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:Arial;"&gt;21&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;甲&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;省道）&lt;/span&gt;&lt;/strong&gt;&lt;/font&gt;&lt;span lang="EN-US" style="FONT-SIZE:9.5pt;FONT-FAMILY:Arial;"&gt;&lt;br&gt;&lt;br&gt;&lt;/span&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:red;FONT-FAMILY:新細明體;"&gt;&lt;font style="BACKGROUND-COLOR:#d0d0d0;"&gt;競賽項目：&lt;/font&gt;&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:Arial;"&gt;&lt;br&gt;&lt;/span&gt;&lt;/strong&gt;&lt;font style="BACKGROUND-COLOR:#d0d0d0;"&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:新細明體;"&gt;　&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span lang="EN-US" style="FONT-SIZE:10pt;COLOR:black;FONT-FAMILY:Arial;"&gt;1. &lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style="FONT-SIZE:10pt;